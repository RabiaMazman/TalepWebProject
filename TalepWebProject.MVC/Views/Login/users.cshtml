@model List<TalepWebProject.MVC.Models.PersonelView>


<!DOCTYPE html>
<html>

<head>
    <meta charset="UTF-8">
    <meta content="width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no" name="viewport">
    <!-- Favicon-->
    <link rel="icon" href="/Tamplate/favicon.ico" type="image/x-icon">

    <!-- Google Fonts -->
    <link href="https://fonts.googleapis.com/css?family=Roboto:400,700&subset=latin,cyrillic-ext" rel="stylesheet" type="text/css">
    <link href="https://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet" type="text/css">

    <!-- Bootstrap Core Css -->
    <link href="/Tamplate/plugins/bootstrap/css/bootstrap.css" rel="stylesheet">

    <!-- Waves Effect Css -->
    <link href="/Tamplate/plugins/node-waves/waves.css" rel="stylesheet" />

    <!-- Animation Css -->
    <link href="/Tamplate/plugins/animate-css/animate.css" rel="stylesheet" />

    <!-- JQuery DataTable Css -->
    <link href="/Tamplate/plugins/jquery-datatable/skin/bootstrap/css/dataTables.bootstrap.css" rel="stylesheet">

    <!-- Custom Css -->
    <link href="/Tamplate/css/style.css" rel="stylesheet">

    <!-- AdminBSB Themes. You can choose a theme from css/themes instead of get all themes -->
    <link href="/Tamplate/css/themes/all-themes.css" rel="stylesheet" />
</head>

<body class="theme-red">


    <section class="content">
        <div class="container-fluid">
            <div class="block-header">
                <h2>
                </h2>
            </div>

            <div class="row clearfix">
                <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
                    <div class="card">
                        <div class="header">
                            <h2>
                                Personnel Listesi
                            </h2>

                        </div>
                        <div class="body">
                            <div class="table-responsive">
                                <table class="table table-bordered table-striped table-hover dataTable js-exportable">
                                    <thead>
                                        <tr>
                                            <th>Personel Adı</th>

                                            <th>Telefon</th>
                                            <th>Email</th>
                                            <th>Yetkisi</th>

                                            <th>İşlemler</th>
                                        </tr>
                                    </thead>
                                    <tfoot>
                                        <tr>

                                            <th>Personel Adı</th>

                                            <th>Telefon</th>
                                            <th>Email</th>
                                            <th>Yetkisi</th>

                                            <th>İşlemler</th>
                                        </tr>
                                    </tfoot>
                                    <tbody>
                                        @foreach (var item in Model)
                                        {

                                            <tr>
                                                <td class="min-width">

                                                    @item.PersonName
                                                </td>


                                                <td class="min-width">
                                                    @item.PersonPhone
                                                </td>

                                                <td class="min-width">
                                                    @item.PersonEmail
                                                </td>

                                                <td class="min-width">
                                                    @item.PersonRolId
                                                </td>



                                                <td class="min-width">
                                                    <a asp-action="UpdatePersonel" asp-route-id="@item.PersonID">Düzenle</a>
                                                </td>
                                            </tr>
                                        }

                                    </tbody>

                                </table>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <!-- #END# Exportable Table -->
        </div>
    </section>

    <!-- Jquery Core Js -->
    <script src="/Tamplate/plugins/jquery/jquery.min.js"></script>

    <!-- Bootstrap Core Js -->
    <script src="/Tamplate/plugins/bootstrap/js/bootstrap.js"></script>

    <!-- Select Plugin Js -->
    <script src="/Tamplate/plugins/bootstrap-select/js/bootstrap-select.js"></script>

    <!-- Slimscroll Plugin Js -->
    <script src="/Tamplate/plugins/jquery-slimscroll/jquery.slimscroll.js"></script>

    <!-- Waves Effect Plugin Js -->
    <script src="/Tamplate/plugins/node-waves/waves.js"></script>

    <!-- Jquery DataTable Plugin Js -->
    <script src="/Tamplate/plugins/jquery-datatable/jquery.dataTables.js"></script>
    <script src="/Tamplate/plugins/jquery-datatable/skin/bootstrap/js/dataTables.bootstrap.js"></script>
    <script src="/Tamplate/plugins/jquery-datatable/extensions/export/dataTables.buttons.min.js"></script>
    <script src="/Tamplate/plugins/jquery-datatable/extensions/export/buttons.flash.min.js"></script>
    <script src="/Tamplate/plugins/jquery-datatable/extensions/export/jszip.min.js"></script>
    <script src="/Tamplate/plugins/jquery-datatable/extensions/export/pdfmake.min.js"></script>
    <script src="/Tamplate/plugins/jquery-datatable/extensions/export/vfs_fonts.js"></script>
    <script src="/Tamplate/plugins/jquery-datatable/extensions/export/buttons.html5.min.js"></script>
    <script src="/Tamplate/plugins/jquery-datatable/extensions/export/buttons.print.min.js"></script>

    <!-- Custom Js -->
    <script src="/Tamplate/js/admin.js"></script>
    <script src="/Tamplate/js/pages/tables/jquery-datatable.js"></script>

    <!-- Demo Js -->
    <script src="/Tamplate/js/demo.js"></script>
</body>

</html>
